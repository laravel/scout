{
    "name": "laravel/scout",
    "description": "Laravel Scout provides a driver based solution to searching your Eloquent models.",
    "keywords": ["laravel", "search", "algolia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": "~5.3",
        "illuminate/database": "~5.3",
        "illuminate/support": "~5.3"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0",
        "algolia/algoliasearch-client-php": "^1.10",
        "elasticsearch/elasticsearch": "^2.2",
        "illuminate/bus": "~5.3",
        "illuminate/pagination": "~5.3",
        "illuminate/queue": "~5.3"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Scout\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "algolia/algoliasearch-client-php": "Required to use the Algolia engine."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
